							 {
							     "panelbody": {
							     	"type":"paneltable",
							     	"dependencynameslist":["AnnexureAA4"],
							     	"value": [
							     		{
							     			"tables": [
										      {
										        "headers": [
										          "A.4",
										          "End user password requirements",
										          "",
										          "Evidence"
										        ],
										        "width": [
										          "1",
										          "6",
										          "3",
										          "2"
										        ],
										        "rows": [
										          [
										            {
										              "type": "text",
										              "value": "A.4.1"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA4A41",
										              "value": "<p><span class=\"panel-span-medium\">The passwords must be encrypted using one-way encryption (SHA-256)</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA4A41Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA4A41Comments",
										              "rows": "2"
										            }
										          ],
										          [
										            {
										              "type": "text",
										              "value": "A.4.2"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA4A42",
										              "value": "<p><span class=\"panel-span-medium\">The system must enforce password aging and change after 3 months</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA4A42Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA4A42Comments",
										              "rows": "2"
										            }
										          ],
										          [
										            {
										              "type": "text",
										              "value": "A.4.3"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA4A43",
										              "value": "<p><span class=\"panel-span-medium\">The system must temporary lock out the user account for 30 minutes after a 5 consecutive unsuccessful login attempts within 10 minutes</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA4A43Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA4A43Comments",
										              "rows": "2"
										            }
										          ],
										          [
										            {
										              "type": "text",
										              "value": "A.4.4"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA4A44",
										              "value": "<p><span class=\"panel-span-medium\">The password must have at least 8 characters with complexity, which requires at least 1 Upper case, 1 lower case, 1 number and 1 special character</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA4A44Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA4A44Comments",
										              "rows": "2"
										            }
										          ],
										          [
										            {
										              "type": "text",
										              "value": "A.4.5"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA4A45",
										              "value": "<p><span class=\"panel-span-medium\">The system must employ checks to ensure user-created passwords meet Information Security complexity requirements.</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA4A45Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA4A45Comments",
										              "rows": "2"
										            }
										          ],
										          [
										            {
										              "type": "text",
										              "value": "A.4.6"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA4A46",
										              "value": "<p><span class=\"panel-span-medium\">Passwords must be stored in a one-way encrypted form.</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA4A46Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA4A46Comments",
										              "rows": "2"
										            }
										          ],
										          [
										            {
										              "type": "text",
										              "value": "A.4.7"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA4A47",
										              "value": "<p><span class=\"panel-span-medium\">The system must provide ability for users to change their passwords, and require users to re-authenticate prior to the password being changed.</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA4A47Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA4A47Comments",
										              "rows": "2"
										            }
										          ],
										          [
										            {
										              "type": "text",
										              "value": "A.4.8"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA4A48",
										              "value": "<p><span class=\"panel-span-medium\">The system must not provide any indication that an account already exists.</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA4A48Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA4A48Comments",
										              "rows": "2"
										            }
										          ],
										          [
										            {
										              "type": "text",
										              "value": "A.4.9"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA4A49",
										              "value": "<p><span class=\"panel-span-medium\">System generated password must consist of patterns which are randomly selected and complex in terms of their length, character and order.</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA4A49Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA4A49Comments",
										              "rows": "2"
										            }
										          ],
										           [
										            {
										              "type": "text",
										              "value": "A.4.10"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA4A410",
										              "value": "<p><span class=\"panel-span-medium\">The system must force change of password for:<br />a. Initially assigned passwords, upon first logon<br />b. Default passwords<br />c. Any temporary passwords used after a reset.</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA4A410Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA4A410Comments",
										              "rows": "2"
										            }
										          ]
										        ]
										      }
										    ]
										}
							     	]
							     }
