										            {
										              "type": "text",
										              "value": "A.5.1"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA5A51",
										              "value": "<p><span class=\"panel-span-medium\">For user accounts that are created manually, the application must require a password reset on first logon.</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA5A51Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA5A51Comments",
										              "rows": "2"
										            }
										          ],
										          [
										            {
										              "type": "text",
										              "value": "A.5.2"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA5A52",
										              "value": "<p><span class=\"panel-span-medium\">Where an automated process is not available, an administrator must be assigned with the responsibility of disabling user accounts (within 24 hours) upon termination of employment.</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA5A52Radio",
										              "options": [
										                "Yes",
										                "No",
										                "N/A"
										              ]
										            },
										            {
										              "type": "textarea",
										              "paramname": "APSAnnexAA5A52Comments",
										              "rows": "2"
										            }
										          ],
										          [
										            {
										              "type": "text",
										              "value": "A.5.3"
										            },
										            {
										              "type": "textandcomments",
													  "id": "APSAnnexAA5A53",
										              "value": "<p><span class=\"panel-span-medium\">>The administrator must perform manual review of accounts every 180 days, and disable any that are no longer required.</span></p>"
										            },
										            {
										              "type": "radio",
										              "paramname": "APSAnnexAA5A53Radio",
										              "options": [
										                "Yes",
										                "No",
